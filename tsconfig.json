{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2021",
    "module": "commonjs",
    "lib": ["es2021"],
    "outDir": "dist",
    "strict": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
